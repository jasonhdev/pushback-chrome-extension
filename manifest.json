{
  "manifest_version": 3,
  "name": "PushBack",
  "version": "1.0",
  "description": "Bring Pushbullet extension back through the use of its API",
  "permissions": [
    "notifications",
    "storage",
    "alarms",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}